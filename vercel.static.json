{
  "rewrites": [
    {
      "source": "/international-investors",
      "destination": "/international-investors.html"
    },
    {
      "source": "/tech-investment", 
      "destination": "/tech-investment.html"
    },
    {
      "source": "/sustainable-investment",
      "destination": "/sustainable-investment.html"
    },
    {
      "source": "/lifestyle-investment",
      "destination": "/lifestyle-investment.html"
    },
    {
      "source": "/nordic-gateway",
      "destination": "/nordic-gateway.html"
    },
    {
      "source": "/docs/(.*)",
      "destination": "/docs/$1"
    },
    {
      "source": "/public/(.*)",
      "destination": "/public/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}